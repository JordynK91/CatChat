<!DOCTYPE html>
<html>
  <head>
    <title>Railsblog2</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
<!-- navbar only presents when user is logged in -->
    <% if @current_user != nil %>
        <nav class="navbar navbar-inverse">      
             <ul class="nav navbar-nav navbar-left">
               <li class="active">
              <%=link_to "Feed", blogs_path %>
              </li>
              <li class = "active">
              <%= link_to "Dashboard", edit_user_path(@current_user)%>
              </li>
            <li class = "active"> 
              <%= link_to'Sign Out', logout_path, method: :destroy %>
            </li>
          </ul>
    </nav>
    <% end %>
  <body>


    <%= yield %>
  </body>
</html>
